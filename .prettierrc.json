{
    "printWidth": 120,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "none",
    "overrides": [
        {
            "files": "*.vue",
            "options": {
                "parser": "vue"
            }
        },
        {
            "files": "*.js",
            "options": {
                "parser": "babel"
            }
        }
    ],
    "vueIndentScriptAndStyle": true
}
